{
  "name": "sick-fits-frontend",
  "version": "2.0.0",
  "description": "Sick Fits! An example React, GraphQL, Next and Apollo",
  "main": "index.js",
  "scripts": {
    "dev": "next -p 7777",
    "build": "next build",
    "start": "next start -p 7777",
    "test": "NODE_ENV=test jest --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.10.4",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@apollo/react-ssr": "^4.0.0",
    "@stripe/react-stripe-js": "^2.7.1",
    "@stripe/stripe-js": "^3.4.0",
    "apollo-upload-client": "^18.0.1",
    "date-fns": "^3.6.0",
    "downshift": "^9.0.6",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "lodash.debounce": "^4.0.8",
    "next": "^14.2.3",
    "next-with-apollo": "^5.3.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-transition-group": "^4.4.5",
    "styled-components": "^6.1.11",
    "waait": "^1.0.5"
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  },
  "//": "This is our babel config, I prefer this over a .babelrc file",
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
